}
.tb{
    font-weight: 600;
}
.owl-next,.owl-prev{
    position: absolute;
            top: 40%;

            padding: 1em;
            font-size: 1.4em;
            font-weight: 700;
            box-shadow: 0 2px 6px 0 rgba(0, 0, 0, 0.3);
            border-radius: 50%;
}
.owl-next{
    right: 0;
}
.owl-prev{
    left: 0;
}
.p-4{
    padding: 4em;
}
.p-2{
    padding: 2em
}
.carousel-mover-handler {
    position: absolute;
    top: 40%;
    z-index: 99;
    width: 2em;
}
</style>



