    <script>
    $.ajax({
        url:"/show/banners/pop_product",
        type:"get",
        success : (res) => {
        for(let i = 0 ;i<res.length;i++){
        $(`#pop_product_${i} > a`)
        .attr('href',`/show/product/${res[i].banner_name_link}`);

        $(`#pop_product_${i} .product-description > a`)
        .attr('href',`/show/product/${res[i].banner_name_link}`);


        $(`#pop_product_${i} .product-img img`)
        .attr('src',`/images/website-img/${res[i].banner_name_img}`)
        $(`#pop_product_${i} .product-description span`).html(res[i].subCategory)
        $(`#pop_product_${i} .product-description a h6`).html(res[i].name)
        $(`#pop_product_${i} .box-compact .cost .small-para.tb`).html("&#8377 "+res[i].price)

            }
        }
    })
    </script>
