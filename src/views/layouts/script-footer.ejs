   <script>

            const checkCart = ()=>{
                $.ajax({
                 url:"/cart/check",
                 type:"get",
                 dataType:"text",
                 success : (res)=>{
                    res = JSON.parse(res)

                    if(res == null || res.length == 0 || res == '[]' ){
                        $('#c-bdge').html(0)
                    }else{
                        $('#c-bdge').html(res[0].numberOfItem)
                    }

                 }
                 })
            }



            checkCart();



            if(typeof errors !== undefined) {
                Object.keys(errors).forEach(err => {
                toastr.success(err, null)
             })}

   </script>
