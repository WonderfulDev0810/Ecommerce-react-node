            $('.navbar.navbar-default.navbar-fixed-top').css({'transform': 'translateY(-7em)'});
            $('.lower-header').css({'position':'fixed','top':"0px"})
        }else{
           $('.navbar.navbar-default.navbar-fixed-top').css({'transform': 'translateY(0px)'});
           $('.lower-header').css({'position':'relative','top':"0px"})

        }
    })
</script>





