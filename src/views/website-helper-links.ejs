<script>
    $(window).on('load',async e => {

    })
   $(window).on('hashchange',e => {
       var hash = window.location.hash != '' ? window.location.hash : '#about';
       console.log('hash :', hash);
       $('.helper-page-content > div').hide()
       $('.helper-page-content > div').filter(hash).show();
       $('.helper-page-handler ul li ').removeClass('hp-handler-active')
       $(`${hash}-handler li `).addClass('hp-handler-active');
   }).trigger('hashchange')

</script>


